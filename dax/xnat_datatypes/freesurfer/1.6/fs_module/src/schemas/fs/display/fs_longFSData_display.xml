	<ViewLink alias="FS_LONGFSDATA_PROJECTS">
		<Mapping TableName="FS_LONGFSDATA_PROJECTS">
			<MappingColumn rootElement="fs:longFSData" fieldElement="fs:longFSData.ID" mapsTo="id"/>
		</Mapping>
	</ViewLink>
	<SQLView name="FS_LONGFSDATA_PROJECTS" sql="SELECT id, '&lt;' || expt.project || '&gt;' || xs_a_concat(', ' || shared.project) AS projects FROM xnat_experimentData expt LEFT JOIN xnat_experimentData_share shared ON expt.id=shared.sharing_share_xnat_experimentda_id LEFT JOIN xdat_meta_element xme ON expt.extension = xme.xdat_meta_element_id WHERE element_name='fs:longFSData' GROUP BY expt.id,expt.project"/>
